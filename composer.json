{
  "name": "tehekone/laravelcursorpagination",
  "description": "Facebook's like cursor pagination for Laravel/Lumen",
  "homepage": "https://github.com/tehekone/laravelcursorpagination",
  "license": "MIT",
  "keywords": [
    "laravel",
    "pagination",
    "api"
  ],
  "authors": [
    {
      "name": "Alexandr Tyurin",
      "email": "tehekone@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "illuminate/support": "5.5.*||5.6.*||5.7.*||5.8.*",
    "illuminate/http": "5.5.*||5.6.*||5.7.*||5.8.*",
    "illuminate/database": "5.5.*||5.6.*||5.7.*||5.8.*"
  },
  "require-dev": {
    "orchestra/testbench": "~3.5",
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "TehekOne\\LaravelCursorPagination\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TehekOne\\LaravelCursorPagination\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TehekOne\\LaravelCursorPagination\\CursorPaginationServiceProvider"
      ],
      "aliases": {
        "CursorPagination": "TehekOne\\LaravelCursorPagination\\CursorPagination"
      }
    }
  }
}